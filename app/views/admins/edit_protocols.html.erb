<h1>Update Protocol</h1>

<%= form_tag({:action => "edit_protocols"}, {:method => "post"}) do %>
	<input type='hidden' name='protocol_id' value=<%= @protocol.id %>
	<br />
	<%= label_tag :name %>
	<%= text_field_tag :name, @protocol.name %>
	<br />
	<%= label_tag :description %>
	<%= text_field_tag :description, @protocol.description %>
	<br />
	<%= label_tag :retire %>
	<%= text_field_tag :retired, @protocol.retired%>
	<br />

	<%= link_to "Cancel" , protocols_path %>
	<%= submit_tag "Finish" %>

<%end%>
