<p id="notice"><%= notice %></p>
<p id="alert"><%= alert %></p>
<h1>Log In</h1>
<%=form_tag log_in_path do%>
	<%=label_tag :username %>
	<%=text_field_tag :username %>
	
	<br />
	<%=label_tag :password %>
	<%=password_field_tag :password %>

	<br />
	<%=label_tag :location %>
	<%=text_field_tag :location %>

	<br />
	<%=submit_tag "Login" %>
<%end%>
