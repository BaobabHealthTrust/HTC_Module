<html>
  <head>
    <%= javascript_include_tag 'jquery-1.3.2.min' %>
    <script>
      var imgMax = "data:image/png;base64,";
      var imgMin = "data:image/png;base64,"

        jQuery(document).ready(function($) {
              __$("img-max-left").src = imgMax;
              __$("img-min-left").src = imgMin;
              __$("img-max-right").src = imgMax;
              __$("img-min-right").src = imgMin;

              __$("img-min-left").style.display = "none";
              __$("img-min-right").style.display = "none";

              panel1 =  __$("rightpanel");
              panel2 = __$("letfpanel");

              __$("img-max-left").onmousedown =  function () {
                     panel1.style.display = "none";                  
                     panel2.style.width = "100%";
                     __$("img-min-left").style.display = "inline";
                     __$("img-max-left").style.display = "none";
                }

              __$("img-min-left").onmousedown =  function () {
                     panel1.style.display = "inline";
                     panel2.style.width = "50%";
                     __$("img-min-left").style.display = "none";
                     __$("img-max-left").style.display = "inline";
                }

                __$("img-max-right").onmousedown =  function () {
                     panel2.style.display = "none";
                     panel1.style.width = "100%";
                     __$("img-min-right").style.display = "inline";
                     __$("img-max-right").style.display = "none";
                }

              __$("img-min-right").onmousedown =  function () {
                     panel2.style.display = "inline";
                     panel1.style.width = "50%";
                     __$("img-min-right").style.display = "none";
                     __$("img-max-right").style.display = "inline";
              }

          });


      </script>
    <style>
      #outer{
      height: 100% ! important;
      width:100%;
}
#letfpanel{
  width:50%;
  height:80% ! important;
  float: left;
}
#rightpanel{
  width: 50%;
  height:80% ! important;
  float: right;
}
#bottom{
  width: 100%;
  border-radius: 10px;
  background-color: grey;
}
.div2{
  background-color: rgba(73, 93,135,0.3);
  border-radius: 60px;
  bottom: 2px;
  height: 80px;
  left: 0;
  position: absolute;
  width: 100%;
  z-index: 120;
}
#rightimg{
  float: right;
}
#leftimg{
  float:left;
}
#lower{
  display: none;
}
    </style>
  </head>
  <body>
    <div id="outer">
   
    <div id="letfpanel">
      <div id="leftimg">
        <img id="img-max-left"  width = '45' height='40'></img>
        <img id="img-min-left"  width = '45' height='40'></img>
      </div>
      <iframe src="/client_status/<%= @partner.person_a %>?config=couple" width="100%" height="800px ! important" ></iframe>
    </div>
   
    <div id="rightpanel">
       <div id="rightimg">
        <img id="img-max-right"  width = '45' height='40'></img>
        <img id="img-min-right"  width = '45' height='40'></img>
      </div>
          <iframe src="/client_status/<%= @partner.person_b %>?config=couple" width="100%" height="800px ! important"></iframe>
    </div>
     <div id="lower">
      <div id="uinav" class="div2">
          <button class="button green" style="margin: 10px 20px 10px 10px; float: right; height: 25px; font-size: 18px;" onmousedown="window.location = '/htcs'">Finish</button>
       </div>
    </div>
    </div>
    
  </body>
</html>
