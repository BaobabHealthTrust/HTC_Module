<html>
<head>
  <script>
      var cancelDestination = "/htcs?tab=my_account";
      var imgUnTick = 'data:image/png;base64,';
      var pencil = "data:image/png;base64,";
      var imgMax = "data:image/png;base64,";
      var imgMin = "data:image/png;base64,"
      var data = {};
      var selects = {};
      var spinner;
      var session_date = new Date();
      session_date.setYear("<%= @session_date.year%>");
      session_date.setMonth("<%= @session_date.month - 1%>");
      session_date.setDate("<%= @session_date.day%>");

  </script>

  <%= stylesheet_link_tag 'panel' %>
  <%= javascript_include_tag 'spin' %>
</head>
